{
  "name": "zod-production",
  "version": "1.0.0",
  "description": "Enhanced Zod validation service with configurable error logging and parsing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
